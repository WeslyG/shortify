{
  "name": "shortify",
  "version": "0.1.1",
  "description": "simply link shorter",
  "private": false,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack --config webpack.dev.js",
    "lint": "eslint app.js src/",
    "updep": "rm -rf package-lock.json yarn.lock && yarn && yarn upgrade && npm i",
    "test": "PORT=3333 MONGO_URL='localhost:27018' mocha --exit --timeout 10000 --require @babel/register --delay ./**/*.spec.js",
    "dockerBuild": "docker build -t $npm_package_dockerLogin/$npm_package_name:$npm_package_version .",
    "dockerPush": "docker push $npm_package_dockerLogin/$npm_package_name:$npm_package_version"
  },
  "author": "WeslyG",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "useBuiltIns": "usage",
          "corejs": {
            "version": 3,
            "proposals": true
          }
        }
      ]
    ]
  },
  "dockerLogin": "weslyg",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bowser": "^2.10.0",
    "crc": "^3.8.0",
    "express": "^4.17.1",
    "geoip-lite": "^1.4.2",
    "mongoose": "^5.9.27",
    "ua-parser-js": "^0.7.21"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.6.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "mocha": "^8.1.0",
    "mongo-unit": "^2.0.1",
    "nodemon-webpack-plugin": "^4.3.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": "^12.14.0",
    "npm": "^6.12.0",
    "yarn": "^1.15.0"
  }
}
